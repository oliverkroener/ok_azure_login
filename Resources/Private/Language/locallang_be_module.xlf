<?xml version="1.0" encoding="UTF-8"?>
<xliff version="1.2">
    <file source-language="en" datatype="plaintext" original="messages" date="2026-02-12T00:00:00Z" product-name="ok_azure_login">
        <body>
            <trans-unit id="mlang_tabs_tab" xml:space="preserve">
                <source>Azure Login</source>
            </trans-unit>
            <trans-unit id="mlang_labels_tablabel" xml:space="preserve">
                <source>Azure Login Configuration</source>
            </trans-unit>
            <trans-unit id="mlang_labels_tabdescr" xml:space="preserve">
                <source>Manage Microsoft Entra ID (Azure AD) credentials per site.</source>
            </trans-unit>
            <trans-unit id="module.title" xml:space="preserve">
                <source>Azure Login Configuration</source>
            </trans-unit>
            <trans-unit id="label.noPageSelected" xml:space="preserve">
                <source>Please select a page from the page tree.</source>
            </trans-unit>
            <trans-unit id="label.noSiteFound" xml:space="preserve">
                <source>No TYPO3 site configuration found for this page. Please create a site configuration first.</source>
            </trans-unit>
            <trans-unit id="label.siteIdentifier" xml:space="preserve">
                <source>Site</source>
            </trans-unit>
            <trans-unit id="label.siteRootPageId" xml:space="preserve">
                <source>Site Root Page ID</source>
            </trans-unit>
            <trans-unit id="label.context.frontend" xml:space="preserve">
                <source>Frontend</source>
            </trans-unit>
            <trans-unit id="label.context.backend" xml:space="preserve">
                <source>Backend</source>
            </trans-unit>
            <trans-unit id="label.context.backend.info" xml:space="preserve">
                <source>Backend login configuration for this site. Each site with valid backend credentials will show a separate login button.</source>
            </trans-unit>
            <trans-unit id="label.enabled" xml:space="preserve">
                <source>Enabled</source>
            </trans-unit>
            <trans-unit id="label.enabled.description" xml:space="preserve">
                <source>Enable or disable this backend login configuration. Disabled configurations will not show a login button.</source>
            </trans-unit>
            <trans-unit id="label.backendLoginLabel" xml:space="preserve">
                <source>Login Button Label</source>
            </trans-unit>
            <trans-unit id="label.backendLoginLabel.description" xml:space="preserve">
                <source>Required. Header shown above the login button on the backend login page (e.g. company name).</source>
            </trans-unit>
            <trans-unit id="label.showLabel" xml:space="preserve">
                <source>Show Label on Login</source>
            </trans-unit>
            <trans-unit id="label.showLabel.description" xml:space="preserve">
                <source>Show the login button label above the sign-in button on the backend login page.</source>
            </trans-unit>
            <trans-unit id="label.tenantId" xml:space="preserve">
                <source>Tenant ID</source>
            </trans-unit>
            <trans-unit id="label.tenantId.description" xml:space="preserve">
                <source>Microsoft Entra Directory (Tenant) ID</source>
            </trans-unit>
            <trans-unit id="label.clientId" xml:space="preserve">
                <source>Client ID</source>
            </trans-unit>
            <trans-unit id="label.clientId.description" xml:space="preserve">
                <source>Application (Client) ID from Azure App Registration</source>
            </trans-unit>
            <trans-unit id="label.clientSecret" xml:space="preserve">
                <source>Client Secret</source>
            </trans-unit>
            <trans-unit id="label.clientSecret.description" xml:space="preserve">
                <source>Client secret value from Azure App Registration</source>
            </trans-unit>
            <trans-unit id="label.clientSecret.existing" xml:space="preserve">
                <source>A client secret is stored (encrypted). Leave empty to keep it, or enter a new value to replace it.</source>
            </trans-unit>
            <trans-unit id="label.redirectUriFrontend" xml:space="preserve">
                <source>Redirect URI (Frontend)</source>
            </trans-unit>
            <trans-unit id="label.redirectUriFrontend.description" xml:space="preserve">
                <source>OAuth callback URL for frontend login (e.g. https://example.com/azure-login/callback)</source>
            </trans-unit>
            <trans-unit id="label.redirectUriBackend" xml:space="preserve">
                <source>Redirect URI (Backend)</source>
            </trans-unit>
            <trans-unit id="label.redirectUriBackend.description" xml:space="preserve">
                <source>OAuth callback URL for backend login (e.g. https://example.com/typo3/azure-login/callback)</source>
            </trans-unit>
            <trans-unit id="warning.encryptionKeyMissing.title" xml:space="preserve">
                <source>Encryption Key Missing</source>
            </trans-unit>
            <trans-unit id="warning.encryptionKeyMissing.body" xml:space="preserve">
                <source>The TYPO3 encryption key ($GLOBALS['TYPO3_CONF_VARS']['SYS']['encryptionKey']) is not set. The client secret cannot be encrypted without it. Please configure the encryption key in the Install Tool before saving credentials.</source>
            </trans-unit>
            <trans-unit id="message.saved.title" xml:space="preserve">
                <source>Configuration Saved</source>
            </trans-unit>
            <trans-unit id="message.saved.body" xml:space="preserve">
                <source>Azure Login configuration has been saved successfully.</source>
            </trans-unit>
            <trans-unit id="message.deleted.title" xml:space="preserve">
                <source>Configuration Deleted</source>
            </trans-unit>
            <trans-unit id="message.deleted.body" xml:space="preserve">
                <source>The backend login configuration has been deleted.</source>
            </trans-unit>
            <trans-unit id="button.addBackendConfig" xml:space="preserve">
                <source>Add Backend Login Configuration</source>
            </trans-unit>
            <trans-unit id="button.backToList" xml:space="preserve">
                <source>Back to list</source>
            </trans-unit>
            <trans-unit id="label.noBackendConfigs" xml:space="preserve">
                <source>No backend login configurations found. Click "Add Backend Login Configuration" to create one.</source>
            </trans-unit>
            <trans-unit id="label.cloneFromBackend" xml:space="preserve">
                <source>Clone from Backend Config</source>
            </trans-unit>
            <trans-unit id="label.cloneFromBackend.description" xml:space="preserve">
                <source>Copy Tenant ID, Client ID, and Client Secret from an existing backend configuration</source>
            </trans-unit>
            <trans-unit id="label.cloneFromBackend.placeholder" xml:space="preserve">
                <source>-- Select a backend configuration --</source>
            </trans-unit>
            <trans-unit id="confirm.clone.title" xml:space="preserve">
                <source>Clone Backend Configuration</source>
            </trans-unit>
            <trans-unit id="confirm.clone.button" xml:space="preserve">
                <source>Clone</source>
            </trans-unit>
            <trans-unit id="confirm.clone.redirectNote" xml:space="preserve">
                <source>Please configure a separate Redirect URI for frontend use. Do not reuse the backend Redirect URI.</source>
            </trans-unit>
            <trans-unit id="label.clientSecret.cloned" xml:space="preserve">
                <source>Client secret will be copied from the backend configuration on save.</source>
            </trans-unit>
            <trans-unit id="confirm.delete.title" xml:space="preserve">
                <source>Delete Configuration</source>
            </trans-unit>
            <trans-unit id="confirm.delete.message" xml:space="preserve">
                <source>Are you sure you want to delete this backend login configuration? This action cannot be undone.</source>
            </trans-unit>
            <trans-unit id="confirm.delete.button" xml:space="preserve">
                <source>Delete</source>
            </trans-unit>
        </body>
    </file>
</xliff>
